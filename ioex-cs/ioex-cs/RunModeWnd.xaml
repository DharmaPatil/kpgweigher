                                    <LineSegment Point="29.09,84.89"/>
                                    <ArcSegment Point="90.90,84.89" Size="120,120" SweepDirection="Clockwise"/>
                                    <LineSegment Point="78.54,122.93"/>
                                    <ArcSegment Point="41.45,122.93" Size="90,90" SweepDirection="Counterclockwise"/>
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </Path.Data>
                </Path>
            </Canvas>
        </ControlTemplate>

        <ControlTemplate x:Key="WeightBar" TargetType="{x:Type Button}">
            <Canvas>
                <Path Stroke="DarkGray" StrokeThickness="2" Fill="Blue" Name="weiboard">
                    <Path.Data>
                        <PathGeometry>
                            <PathGeometry.Figures>
                                <PathFigure StartPoint="29.09,84.89" IsClosed="True">
                                    <LineSegment Point="4.37,8.8"/>
                                    <ArcSegment Point="115.62,8.8" Size="180,180" SweepDirection="Clockwise"/>
                                    <LineSegment Point="90.90,84.89"/>
                                    <ArcSegment Point="29.09,84.89" Size="120,120" SweepDirection="Counterclockwise"/>
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </Path.Data>
                </Path>
            </Canvas>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver"  Value = "True">
                    <Setter TargetName="weiboard" Property = "Fill" Value = "Blue"/>
                </Trigger>
            </ControlTemplate.Triggers>

        </ControlTemplate>
                


        
